<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net configSource="Log4net.config" />

  <connectionStrings>
    <add name="MongoDB" connectionString="mongodb://localhost/fsharpnews" />
  </connectionStrings>

  <!-- Production settings -->
  <!--<appSettings configSource="Production.Settings.config" />-->

  <!-- Developer settings -->
  <appSettings >
    <add key="StackExchangeApiKey" value="" />
    <add key="TwitterConsumerKey" value="" />
    <add key="TwitterConsumerSecret" value="" />
    <add key="TwitterAccessToken" value="" />
    <add key="TwitterAccessTokenSecret" value="" />
  </appSettings>
    <add key="TwitterStreamingApiUrl" value="https://stream.twitter.com/1.1/" />

  <system.net>
    <defaultProxy useDefaultCredentials="true" />
  </system.net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="FSharp.Core" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.3.1.0" newVersion="4.3.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="MongoDB.Bson" publicKeyToken="f686731cfb9cc103" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.8.3.9" newVersion="1.8.3.9" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>